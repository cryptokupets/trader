<mvc:View controllerName="ck.trader.controller.Main" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:l="sap.ui.layout" 
	xmlns:core="sap.ui.core" 
	xmlns="sap.m">
	<Page title="Apps">
		<content>
			<l:VerticalLayout width="100%">
				<l:BlockLayout>
					<l:BlockLayoutRow>
						<l:BlockLayoutCell titleAlignment="Center">
							<l:titleLink>
								<Link text="Analysis" 
									href="/#/analysis/input"/>
							</l:titleLink>
						</l:BlockLayoutCell>
						<l:BlockLayoutCell titleAlignment="Center">
							<l:titleLink>
								<Link text="Sessions" 
									href="/#/sessions"/>
							</l:titleLink>
						</l:BlockLayoutCell>
					</l:BlockLayoutRow>
				</l:BlockLayout>
			</l:VerticalLayout>
		</content>
	</Page>
</mvc:View>
