{
  "sap.app": {
    "id": "ck.trader",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "default": {
        "uri": "/odata/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5"
  },
  "sap.ui5": {
    "autoPrefixId": true,
    "models": {
      "": {
        "dataSource": "default",
        "settings": {
          "autoExpandSelect": false,
          "groupId": "$direct",
          "synchronizationMode": "None",
          "operationMode": "Server"
        }
      },
      "draft": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "view": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "device": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "rootView": {
      "viewName": "ck.trader.view.App",
      "type": "XML"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlId": "app",
        "viewType": "XML",
        "controlAggregation": "pages",
        "viewPath": "ck.trader.view",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "main",
          "target": "main"
        },
        {
          "pattern": "analysis/form",
          "name": "analysisForm",
          "target": "analysisForm"
        },
        {
          "pattern": "analysis/show",
          "name": "analysisShow",
          "target": "analysisShow"
        },
        {
          "pattern": "sessions",
          "name": "sessions",
          "target": "sessions"
        },
        {
          "pattern": "session/{id}",
          "name": "session",
          "target": "session"
        }
      ],
      "targets": {
        "main": {
          "viewName": "Main",
					"viewLevel": 0
        },
        "analysisForm": {
          "viewName": "AnalysisForm",
          "viewLevel": 1
        },
        "analysisShow": {
          "viewName": "AnalysisShow",
          "viewLevel": 2
        },
        "sessions": {
          "viewName": "Sessions",
          "viewLevel": 1
        },
        "session": {
          "viewName": "Session",
          "viewLevel": 2
        }
      }
    }
  }
}
